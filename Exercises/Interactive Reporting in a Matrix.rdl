  <DataSets>
    <DataSet Name="Transactions">
      <Query>
        <DataSourceName>WideWorldImporters</DataSourceName>
        <CommandText>	SELECT 
 	--Row Groups:
		 CityID                      = AC.CityID 
		,CityName                    = AC.CityName
		,StateProvinceID             = ASP.StateProvinceID
		,StateProvince               = ASP.StateProvinceName  
	--Column Groups
		,TransactionDate_Year        = YEAR(SCT.TransactionDate)
		,TransactionDate_MonthID     = MONTH(SCT.TransactionDate)
		,TransactionDate_Month       = DATENAME(MONTH,SCT.TransactionDate)
	--Details/Aggregates:
		,TransactionAmount           = SUM(SCT.TransactionAmount)
		,Cnt                         = SUM(1)
	FROM Sales.CustomerTransactions SCT
	LEFT JOIN Sales.Customers SC ON SCT.CustomerID = SC.CustomerID
	LEFT JOIN Application.Cities AC ON AC.CityID = SC.PostalCityID
	LEFT JOIN Application.StateProvinces ASP ON AC.StateProvinceID = ASP.StateProvinceID
	LEFT JOIN Application.Countries ACT ON ACT.CountryID = ASP.CountryID
	LEFT JOIN Application.TransactionTypes ATT ON ATT.TransactionTypeID = SCT.TransactionTypeID
	
	GROUP BY 
	 AC.CityID 
	,AC.CityName
	,ASP.StateProvinceID
	,ASP.StateProvinceName 
	,YEAR(SCT.TransactionDate)
	,MONTH(SCT.TransactionDate)
	,DATENAME(MONTH,SCT.TransactionDate)
	</CommandText>
      </Query>
